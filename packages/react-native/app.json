{
  "expo": {
    "name": "f0-react-native",
    "slug": "f0-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "f0reactnative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      ["expo-router", {
        "root": "./playground/app"
      }],
      [
      "expo-font",
        {
          "fonts": [
            "./assets/fonts/factorial.ttf",
            "./assets/fonts/Inter/InterNormal.ttf",
            "./assets/fonts/Inter/InterMedium.ttf",
            "./assets/fonts/Inter/InterSemiBold.ttf",
            "./assets/fonts/Inter/InterBold.ttf",
            "./assets/fonts/OxaniumMedium.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
