{
  "name": "@factorialco/f0-react-native",
  "version": "0.22.0",
  "type": "module",
  "description": "React Native components for F0 Design System",
  "main": "expo-router/entry",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "source": "./src/index.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": {
        "types": "./lib/typescript/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "react-native": "./lib/module/index.js",
      "default": "./lib/module/index.js"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "import": "./src/icons/index.ts",
      "require": "./src/icons/index.ts",
      "react-native": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    "./icons/app": {
      "types": "./src/icons/app/index.ts",
      "import": "./src/icons/app/index.ts",
      "require": "./src/icons/app/index.ts",
      "react-native": "./src/icons/app/index.ts",
      "default": "./src/icons/app/index.ts"
    },
    "./icons/modules": {
      "types": "./src/icons/modules/index.ts",
      "import": "./src/icons/modules/index.ts",
      "require": "./src/icons/modules/index.ts",
      "react-native": "./src/icons/modules/index.ts",
      "default": "./src/icons/modules/index.ts"
    },
    "./src/icons/app": {
      "types": "./src/icons/app/index.ts",
      "import": "./src/icons/app/index.ts",
      "require": "./src/icons/app/index.ts",
      "react-native": "./src/icons/app/index.ts",
      "default": "./src/icons/app/index.ts"
    },
    "./package.json": "./package.json",
    "./theme.css": {
      "import": "./src/styles/theme.css",
      "require": "./src/styles/theme.css",
      "react-native": "./src/styles/theme.css",
      "default": "./src/styles/theme.css"
    },
    "./expo-router/entry": {
      "import": "./expo-router/entry.js",
      "require": "./expo-router/entry.js",
      "react-native": "./expo-router/entry.js",
      "default": "./expo-router/entry.js"
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "prebuild": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json','utf8'));pkg._originalMain=pkg.main;pkg.main='./lib/module/index.js';fs.writeFileSync('package.json',JSON.stringify(pkg,null,2))\"",
    "build": "bob build",
    "postbuild": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json','utf8'));if(pkg._originalMain){pkg.main=pkg._originalMain;delete pkg._originalMain;fs.writeFileSync('package.json',JSON.stringify(pkg,null,2))}\" && test -d lib/typescript/react-native/package && rm -rf lib/typescript/react-native/package || true",
    "prepare": "bob build",
    "lint": "eslint -c eslint.config.mjs . --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint -c eslint.config.mjs . --fix --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --coverageReporters=text --coverageReporters=lcov",
    "generate-icons": "rm -fR src/icons/app/* src/icons/modules/* && run-p generate-app-icons generate-module-icons && find src/icons -name '*.tsx' -exec sed -i.bak 's/SVGSVGElement/Svg/g; s/ xmlns=\"http:\\/\\/www.w3.org\\/2000\\/svg\"//g; s/ strokeWidth={[^}]*}//g; s/ strokeWidth=\"[^\"]*\"//g' {} + && find src/icons -name '*.bak' -delete && pnpm run prettier:format",
    "generate-app-icons": "npx @svgr/cli --out-dir src/icons/app --native --svg-props className={props.className} ../../packages/core/assets/icons/app",
    "generate-module-icons": "npx @svgr/cli --out-dir src/icons/modules --native --svg-props className={props.className} ../../packages/core/assets/icons/modules",
    "prettier:format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit"
  },
  "keywords": [
    "factorial",
    "design-system",
    "react-native",
    "components"
  ],
  "author": "Factorial",
  "license": "MIT",
  "peerDependencies": {
    "date-fns": "^3.6.0",
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "^3.19.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-svg": "^15.12.1",
    "react-native-worklets-core": "^1.6.2",
    "tailwindcss": "^4.1.18",
    "uniwind": "^1.2.7",
    "twemoji-parser": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-worklets-core": {
      "optional": true
    }
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "cva": "1.0.0-beta.3",
    "expo-clipboard": "~8.0.8",
    "lodash": "^4.17.21",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@svgr/cli": "^8.1.0",
    "@types/lodash": "^4.17.16",
    "@types/twemoji-parser": "^13.1.4",
    "globals": "^16.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^4.1.18",
    "uniwind": "^1.2.7",
    "tailwindcss-animate": "^1.0.7",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "~54.0.32",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.22",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.4.0",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.10",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.18.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "expo-updates": "^29.0.16",
    "jest": "^30.2.0",
    "react-test-renderer": "^19.1.0",
    "react-native-builder-bob": "^0.40.12",
    "typescript": "~5.9.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true,
          "configFile": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}