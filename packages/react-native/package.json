{
  "name": "@factorialco/factorial-one-react-native",
  "version": "0.0.1",
  "type": "module",
  "description": "React Native components for Factorial One Design System",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./tailwind.config": "./tailwind.config.js"
  },
  "files": [
    "dist",
    "tailwind.config.js"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint -c eslint.config.mjs . --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "pnpm run lint -- --fix",
    "test": "vitest run"
  },
  "keywords": [
    "factorial",
    "design-system",
    "react-native",
    "components"
  ],
  "author": "Factorial",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-native": "^0.76.6",
    "typescript": "^5.7.2",
    "nativewind": "^4.1.23",
    "tailwindcss": "3.3.5"
  },
  "devDependencies": {
    "@factorialco/factorial-one-core": "workspace:*",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "react": "^18.2.0",
    "react-native": "^0.76.6",
    "react-test-renderer": "^18.2.0",
    "vitest": "^3.0.7",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "globals": "^16.0.0",
    "nativewind": "^4.1.23",
    "tailwindcss": "3.3.5",
    "tailwindcss-animate": "^1.0.7"
  }
}
