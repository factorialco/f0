{
  "name": "react-native-playground",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "dev:ios": "expo run:ios",
    "dev:android": "expo run:android",
    "build:dev:ios": "eas build --profile development --platform ios --local",
    "build:dev:android": "eas build --profile development --platform android --local",
    "prebuild": "expo prebuild --clean",
    "postinstall": "pnpm --filter @factorialco/f0-react-native build && npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "storybook-generate": "sb-rn-get-stories",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint -c eslint.config.mjs . --report-unused-disable-directives --max-warnings 0",
    "lint-fix": "eslint -c eslint.config.mjs . --fix --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@factorialco/f0-core": "workspace:*",
    "@factorialco/f0-react-native": "workspace:*",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/avatar": "~1.1.0",
    "@rn-primitives/portal": "~1.1.0",
    "@rn-primitives/progress": "~1.1.0",
    "@rn-primitives/slot": "~1.1.0",
    "@rn-primitives/tooltip": "~1.1.0",
    "@rn-primitives/types": "~1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "expo": "54.0.27",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-linking": "~8.0.10",
    "expo-navigation-bar": "~5.0.10",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "lucide-react-native": "^0.378.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.2.3",
    "react-native": "0.81.5",
    "react-native-reanimated": "3.19.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets-core": "^1.6.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "twemoji-parser": "^14.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.6.0",
    "@react-native-community/slider": "4.5.5",
    "@storybook/addon-ondevice-actions": "^10.1.11",
    "@storybook/addon-ondevice-controls": "^10.1.11",
    "@storybook/addon-ondevice-notes": "^10.1.11",
    "@storybook/react-native": "^10.1.11",
    "@types/react": "~19.1.10",
    "babel-loader": "^8.4.1",
    "react-native-gesture-handler": "~2.28.0",
    "typescript": "~5.9.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-reanimated"
      ]
    },
    "doctor": {
      "reactNativeDirectoryCheck": {
        "enabled": true,
        "exclude": [
          "react-native-reanimated"
        ]
      }
    }
  },
  "private": true
}
