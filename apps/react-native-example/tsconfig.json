{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-native",
    "paths": {
      "~/*": [
        "*"
      ],
      "@factorialco/factorial-one-react-native": [
        "../../packages/react-native/src"
      ],
      "@factorialco/factorial-one-core": [
        "../../packages/core/src"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}